@using OJS.Common.Models
@model ContestQuestionType

@(Html.Kendo()
    .DropDownListFor(m => m)
    .BindTo(EnumConverter.GetSelectListItems<ContestQuestionType>())
    .Events(e => e.Open("Ojs.KendoControls.DropDownList.alignDropDownToInput"))
    .SelectedIndex((int)Model - 1)
    .HtmlAttributes(new { @class = "pull-left full-kendo-editor" }))